<head>

    <!-- Basic -->
    <meta charset="UTF-8">

    <title>@ViewBag.Title - C POS</title>
    <meta name="keywords" content="POS, Point of Sale" />
    <meta name="description" content="POS System">
    <meta name="author" content="Ayaat">
    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <!-- Web Fonts  -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css">
    
    <!-- Vendor CSS -->
    <link rel="stylesheet" href="~/Content/assets/vendor/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="~/Content/assets/vendor/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="~/Content/assets/vendor/magnific-popup/magnific-popup.css" />
    @*<link rel="stylesheet" href="~/Content/assets/vendor/bootstrap-datepicker/css/datepicker3.css" />*@
    <!-- Report Style -->
    <link rel="stylesheet" href="~/Content/assets/stylesheets/report.css" />
    <!-- Specific Page Vendor CSS -->
    <link rel="stylesheet" href="~/Content/assets/vendor/jquery-ui/css/ui-lightness/jquery-ui-1.10.4.custom.css" />
    <link rel="stylesheet" href="~/Content/assets/vendor/pnotify/pnotify.custom.css" />

    <!-- GRID CSS-->
    <link href="~/Content/assets/javascripts/ui-grid/4.4.7/ui-grid.css" rel="stylesheet"/>
    <link href="~/Content/assets/ng-source/ng-vendor/ng-select2/select2.css" rel="stylesheet"/>

    @if (SoftifyFoodPOSNew.Models.clsCommon.ControllerNameP == "Dashboard")
    {
        <link rel="stylesheet" href="~/Content/assets/vendor/bootstrap-multiselect/bootstrap-multiselect.css" />
        <link rel="stylesheet" href="~/Content/assets/vendor/morris/morris.css" />
        <link rel="stylesheet" href="~/Content/assets/vendor/chartist/chartist.css" />
    }
    
    @if (SoftifyFoodPOSNew.Models.clsCommon.ActionNameP == "RptSalesStatement")
    {
        <link href="~/Content/assets/vendor/bootstrap-datetimepicker/bootstrap-datetimepicker.min.css" rel="stylesheet" />
    }
    else
    {
        <link rel="stylesheet" href="~/Content/assets/vendor/bootstrap-datepicker/css/datepicker3.css" />
    }

    <link rel="stylesheet" href="~/Content/assets/vendor/select2/select2.css" />
    <link rel="stylesheet" href="~/Content/assets/vendor/dropzone/css/basic.css" />
    
    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/Content/assets/stylesheets/theme.css" />
    @*<link rel="stylesheet" href="~/Content/assets/stylesheets/theme_new.css" />*@
    <!-- Skin CSS -->
    <link rel="stylesheet" href="~/Content/assets/stylesheets/skins/default.css" />
    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="~/Content/assets/stylesheets/theme-custom.css">
    <!-- Head Libs -->
    <script src="~/Content/assets/vendor/modernizr/modernizr.js"></script>
 
    <!-- gridJS -->
    @Scripts.Render("~/bundles/gridJs")
    <!-- custom app/directive -->
    <link href="~/Content/assets/vendor/bootstrap-fileupload/bootstrap-fileupload.min.css" rel="stylesheet" />

    <script src="~/Content/assets/ng-source/ng-app/masterApp.js"></script>
    <script src="~/Content/assets/ng-source/ng-directive/ng-directive.js"></script>
    <script src="~/Content/assets/vendor/ui-bootstrap/ui-bootstrap-tpls.js"></script>

</head>

